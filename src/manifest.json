{
  "manifest_version": 3,
  "name": "Bandcamp label Dashboard",
  "version": "1.0.0",
  "description": "Bandcamp label Dashboard",
  "permissions": [
    "tabs",
    "downloads"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "action": {
    "default_title": "Bandcamp label",
    "default_popup": "popup.html" 
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "jquery-2.1.1.min.js",
        "label.js",
        "user.js"
      ]
    }
  ]
}
